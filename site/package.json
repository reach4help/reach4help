{
  "name": "site",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "gatsby develop",
    "serve": "yarn build && gatsby serve",
    "lint": "eslint ./src && prettier --check \"./src/**/*.{js,jsx}\"",
    "lint:fix": "eslint ./src --fix && prettier --write \"./src/**/*.{js,jsx}\"",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "test:ci": "exit 0",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "d3": "^6.3.1",
    "fs-extra": "^9.0.1",
    "gatsby": "^5.13.3",
    "gatsby-image": "^2.4.5",
    "gatsby-plugin-manifest": "^2.4.9",
    "gatsby-plugin-react-helmet": "^3.3.2",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-plugin-webfonts": "^1.1.3",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^2.8.12",
    "gatsby-transformer-sharp": "^2.5.3",
    "i18next": "^19.4.5",
    "i18next-browser-languagedetector": "^6.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-datamaps": "^0.4.1",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-i18next": "^11.5.0",
    "react-icons": "^4.1.0",
    "react-select": "^3.1.0",
    "react-world-flags": "^1.4.0",
    "smooth-scroll": "^16.1.3",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "gatsby-plugin-root-import": "^2.0.5",
    "lint-staged": "^10.2.7",
    "prettier": "2.0.5"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reach4help/reach4help/site"
  },
  "bugs": {
    "url": "https://github.com/reach4help/reach4help/issues"
  }
}
