{
  "name": "map",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@reach4help/model": "^0.0.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "@types/googlemaps": "^3.39.3",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-icons": "^3.0.0",
    "@types/react-select": "^3.0.12",
    "firebase": "^7.14.2",
    "intl-messageformat": "^9.3.8",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.3.0",
    "react-gtm-module": "^2.0.11",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.2.0",
    "react-scripts": "3.4.3",
    "react-select": "^3.1.0",
    "styled-components": "^5.0.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "clean": "rm -rf build",
    "build-deps": "yarn --cwd ../model run build",
    "build-map": "cross-env react-scripts build",
    "build": "yarn run build-deps && yarn run build-map",
    "build-netlify": "yarn run build-deps && yarn run build-map",
    "test": "exit 0",
    "test:ci": "exit 0",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx && prettier --check \"./**/*.{ts,tsx}\"",
    "lint:fix": "eslint . --fix --ext .ts,.tsx && prettier --write \"./**/*.{ts,tsx}\"",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint . --ext .ts,.tsx",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/react-gtm-module": "^2.0.0",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1"
  }
}
